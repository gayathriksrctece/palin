import java.io.*;
import java.util.*;
class Palindrome
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int i=n;
int rem=0,rev=0;
if(i>0)
{
while(i>0)
{
rem=i%10;
rev=rev*10+rem;
i=i/10;
}
if(rev==n)
{
System.out.println("It is palindrome");
}
else
{
System.out.println("It is not a palindrome");
}
}
else
{
System.out.print("Invalid input");
}
}
}
